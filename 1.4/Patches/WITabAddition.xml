<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>Defs/WorldObjectDef[defName ="Outpost_Prison"]</xpath>
				<order>Append</order>
				<value>
					<inspectorTabs>
						<li>VOEAdditionalOutposts.WITab_Outpost_Prison</li>
					</inspectorTabs>
				</value>
			</li>
		</operations>
	</Operation>
	
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Ideology</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/WorldObjectDef[defName ="Outpost_Church"]</xpath>
					<order>Append</order>
					<value>
						<inspectorTabs>
							<li>VOEAdditionalOutposts.WITab_Outpost_Church</li>
						</inspectorTabs>
					</value>
				</li>
			</operations>
		</match>
	</Operation>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biotech</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/WorldObjectDef[defName ="Outpost_Educational_Center"]</xpath>
					<order>Append</order>
					<value>
						<inspectorTabs>
							<li>VOEAdditionalOutposts.WITab_Outpost_Educational_Center</li>
						</inspectorTabs>
					</value>
				</li>
			</operations>
		</match>
	</Operation>

</Patch>